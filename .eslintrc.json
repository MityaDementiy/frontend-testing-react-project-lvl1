{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["eslint:recommended","airbnb"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off", 
        "no-param-reassign": "off",
        "treatUndefinedAsUnspecified": false
    },
    "plugins": ["@babel"]
}